
{{#each blogs as |blog|}}
  <div class="blog">
    <div class="">
      <h2>
        <p>{{blog.title}}</p>
      </h2>
      <p>
      <p>Created by
        {{blog.user.name}}
        on
        {{{date_created}}}</p>
    </div>
    <div class="">
      <p>
        {{blog.text}}
      </p>
    </div>
  </div>
{{/each}}




<script src="/js/homepage.js"></script>